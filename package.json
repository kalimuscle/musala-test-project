{
    "private": true,
    "name": "musala-test-project",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
        "dev": "preact watch --template src/template.html",
        "build": "preact build --no-prerender"
    },
    "eslintConfig": {
        "extends": "preact",
        "rules": {
            "indent": "off",
            "comma-dangle": "off",
            "react/jsx-no-bind": "off",
            "react/sort-comp": "off",
            "brace-style": [
                "error",
                "1tbs"
            ],
            "react/jsx-indent-props": [
                2,
                4
            ]
        }
    },
    "eslintIgnore": [
        "build/*"
    ],
    "devDependencies": {
        "node-sass": "^4.14.1",
        "postcss": "^8.3.6",
        "preact-cli": "^3.2.2",
        "preact-cli-svg-loader": "^1.1.0",
        "preact-cli-tailwind": "^3.0.0",
        "preact-habitat": "^3.3.0",
        "preact-render-spy": "^1.3.0",
        "preact-render-to-string": "^5.1.19",
        "sass-loader": "^10.2.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-preact-pure": "^2.0.0",
        "eslint": "^6.0.1",
        "eslint-config-preact": "^1.1.0",
        "jest": "^24.9.0",
        "jest-preset-preact": "^1.0.0",
        "sirv-cli": "1.0.3"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.5.0",
        "autoprefixer": "^10.3.4",
        "axios": "^0.26.1",
        "css-vars-ponyfill": "^2.3.2",
        "date-fns": "^2.15.0",
        "desvg": "^1.0.2",
        "dompurify": "^2.2.6",
        "i18nline": "^2.0.1",
        "isomorphic-dompurify": "^0.15.0",
        "lodash": "^4.17.21",
        "preact": "^10.5.14",
        "preact-context": "^1.1.4",
        "preact-i18nline": "^2.0.0",
        "preact-router": "^3.2.1",
        "query-string": "^6.13.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.5",
        "react-use-wizard": "^2.1.2",
        "redux": "^4.1.1",
        "redux-devtools-extension": "^2.13.9",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "tailwindcss": "^2.2.15",
        "tslib": "^2.3.1",
        "uuid": "^8.3.2",
        "whatwg-fetch": "^3.4.0"
    }
}
